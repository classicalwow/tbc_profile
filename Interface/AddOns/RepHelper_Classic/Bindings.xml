<Bindings>
	<Binding name="SHOWCONFIG" header="REPHELPER" runOnUp="true">
		if ( keystate == "down" ) then
			RPH_ToggleConfigWindow()
		end
	</Binding>
	<Binding name="SHOWDETAILS" runOnUp="true">
		if ( keystate == "down" ) then
			RPH_ToggleDetailWindow()
		end
	</Binding>
</Bindings>
