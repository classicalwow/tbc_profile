					["custom"] = "Minimap.LR_ATTACK_INRANGE = 0\nMinimap.LR_DOT_VISIBLE = false\nMinimap.LR_DOT_X = 0\nMinimap.LR_DOT_Y = 0\n\nMinimap.LR_WGSJ_X = 0\nMinimap.LR_DCSJ_X = 60\nMinimap.LR_ASSJ_X = 120\nMinimap.LR_RANGE_POINTER_X = -90\n\nMinimap.LR_CASTING_MS = false\nMinimap.LR_LAST_MS_TIME = 0\n\nMinimap.LR_Mode = '5511'\nMinimap.LR_SPELL_ARR = {}\nMinimap.LR_Weaving_X = 0\n\nMinimap.LR_Last_Spell = ''\nMinimap.LR_Last_Spell_Time = 0\n\nlocal spell_arr = {'LR_WGSJ_X', 'LR_DCSJ_X', 'LR_ASSJ_X'}\n\nMinimap[spell_arr[tonumber(aura_env.config.spell1)]] = 0\nMinimap[spell_arr[tonumber(aura_env.config.spell2)]] = 60\nMinimap[spell_arr[tonumber(aura_env.config.spell3)]] = 120\n\nMinimap.LR_ICON_X_TABLE = {\n    LR_WGSJ = -60 - 150 + Minimap.LR_WGSJ_X,\n    LR_DCSJ = -60 - 150 + Minimap.LR_DCSJ_X,\n    LR_ASSJ = -60 - 150 + Minimap.LR_ASSJ_X\n}\n\nMinimap.LR_Player = {\n    now = GetTime(),\n    DCSJTime = 0,\n    ASSJTime = 0,\n    gcd = 0,\n    attackTotal = 3.5,\n    attackRelease = 0,\n    shootTotal = 2.9,\n    shootRelease = 0\n}\n\nMinimap.LR_Reset = function()\n    Minimap.LR_DOT_VISIBLE = false\n    Minimap.LR_ATTACK_INRANGE = 0\n    WeakAuras.GetRegion(\"weaving_bar\"):SetAlpha(0)\nend\n\nMinimap.LR_GetCD = function(name)\n    local a, b = GetSpellCooldown(name)\n    local cd = 0\n    if a ~= 0 then\n        cd = a + b - GetTime()\n    end\n    return cd\nend\n\nlocal closeTime = 0\nMinimap.LR_UpdateWeaving = function(skill, delay)\n\n    if not aura_env.config.weaving_shown then\n        WeakAuras.GetRegion(\"weaving_bar\"):SetAlpha(0)\n        return\n    end\n\n    local spell, _, _, _, endTime = UnitCastingInfo(\"player\")\n    local now = GetTime()\n    local gcd = Minimap.LR_Player.gcd\n    local total = Minimap.LR_Player.shootTotal\n    local release = Minimap.LR_Player.shootRelease\n    local start = Minimap.PLAYER_SHOOT_START_TIME\n    local region = WeakAuras.GetRegion(\"weaving_bar\")\n\n    if (Minimap.LR_Mode == '5511' or Minimap.LR_Mode == '5611') and\n        (skill == 'LR_ASSJ' or skill == 'LR_DCSJ') and Minimap.LR_DOT_Y < 50 and\n        Minimap.LR_Player.attackRelease <= release then\n        local finishTime = delay + 1.5\n        local time = finishTime - release\n        local per = time / total\n        region:SetAlpha(1)\n        region:SetWidth(per * 180)\n        region:SetPoint(\"LEFT\", -89, 38)\n        closeTime = GetTime() + release + time + 0.2\n    elseif Minimap.LR_Mode == '11' and spell and Minimap.LR_Player.attackRelease +\n        now < endTime then\n        endTime = endTime / 1000\n        local startX = (endTime - start) / total * 180 - 89\n        local endX = Minimap.LR_RANGE_POINTER_X - 89\n        local width = endX - startX\n        if width > 0 then\n            local x = (endTime - start) / total * 180 - 87\n            region:SetAlpha(1)\n            region:SetWidth(width)\n            region:SetPoint(\"LEFT\", startX, 38)\n            closeTime = endTime + 0.6\n        end\n    elseif GetTime() > closeTime then\n        WeakAuras.GetRegion(\"weaving_bar\"):SetAlpha(0)\n    end\nend\n\nMinimap.LR_LastAttackRelease = 0\nMinimap.LR_LastAttackRelease_Time = 0\nMinimap.LR_GetAttackRelease = function(name)\n    local release = Minimap.PLAYER_ATTACK_RELEASE\n    local now = GetTime()\n    if release == 0 or release ~= Minimap.LR_LastAttackRelease then\n        Minimap.LR_LastAttackRelease = release\n        Minimap.LR_LastAttackRelease_Time = now\n        return release\n    else\n        local dis = now - Minimap.LR_LastAttackRelease_Time\n        return release - dis\n    end\nend\n\nMinimap.LR_LastShootRelease = 0\nMinimap.LR_LastShootRelease_Time = 0\nMinimap.LR_GetShootRelease = function(name)\n    local release = Minimap.PLAYER_SHOOT_RELEASE\n    local now = GetTime()\n    if release == 0 or release ~= Minimap.LR_LastShootRelease then\n        Minimap.LR_LastShootRelease = release\n        Minimap.LR_LastShootRelease_Time = now\n        return release\n    else\n        local dis = now - Minimap.LR_LastShootRelease_Time\n        return release - dis\n    end\nend\n\nMinimap.LR_GetFuturePlayer = function(nowPlayer, delay)\n    local player = {}\n    player.now = nowPlayer.now + delay\n    player.DCSJTime = nowPlayer.DCSJTime - delay\n    if player.DCSJTime < 0 then\n        player.DCSJTime = 0\n    end\n\n    player.ASSJTime = nowPlayer.ASSJTime - delay\n    if player.ASSJTime < 0 then\n        player.ASSJTime = 0\n    end\n\n    player.gcd = nowPlayer.gcd - delay\n    if player.gcd < 0 then\n        player.gcd = 0\n    end\n\n    player.attackTotal = nowPlayer.attackTotal\n    player.attackRelease = nowPlayer.attackRelease - delay\n    if player.attackRelease < 0 then\n        player.attackRelease = player.attackTotal + player.attackRelease\n    end\n\n    player.shootTotal = nowPlayer.shootTotal\n    player.shootRelease = nowPlayer.shootRelease - delay\n    if player.shootRelease < 0 then\n        player.shootRelease = player.shootTotal + player.shootRelease\n    end\n    return player\nend\n\nMinimap.LR_CheckFunc = function(player)\n    local now = player.now\n    local DCSJTime = player.DCSJTime\n    local ASSJTime = player.ASSJTime\n    local gcd = player.gcd\n    local attackTotal = player.attackTotal\n    local attackRelease = player.attackRelease\n    local shootTotal = player.shootTotal\n    local shootRelease = player.shootRelease\n\n    if gcd > 0 then\n        return\n    end\n\n    Minimap.LR_isWarning = false\n    if shootTotal > 2.33 then\n        -- 5411\n        Minimap.LR_Mode = '5411'\n        if Minimap.LR_CASTING_MS then\n            return\n        end\n        if shootRelease == 0 then\n            return 'LR_WGSJ'\n        end\n        if DCSJTime == 0 then\n            return 'LR_DCSJ'\n        end\n        if ASSJTime == 0 and now - Minimap.LR_LAST_MS_TIME > 4 and now -\n            Minimap.LR_LAST_MS_TIME < 9 then\n            return 'LR_ASSJ'\n        end\n        return 'LR_WGSJ'\n    elseif shootTotal > 1.95 then\n        -- 5511\n        Minimap.LR_Mode = '5511'\n        if Minimap.WGTotalTime < shootRelease then\n            return 'LR_WGSJ'\n        end\n        if shootRelease < Minimap.autoCastTime then\n            Minimap.LR_isWarning = true\n            return 'LR_WGSJ'\n        end\n        if DCSJTime == 0 and shootRelease > Minimap.autoCastTime and\n            shootRelease < shootTotal / 2 then\n            return 'LR_DCSJ'\n        end\n        if ASSJTime == 0 and shootRelease > Minimap.autoCastTime and\n            shootRelease < shootTotal / 2 then\n            return 'LR_ASSJ'\n        end\n    elseif shootTotal > 1.74 and aura_env.config.weaving_shown then\n        -- 5511\n        Minimap.LR_Mode = '5511'\n        if Minimap.WGTotalTime < shootRelease then\n            return 'LR_WGSJ'\n        end\n        if shootRelease < Minimap.autoCastTime then\n            Minimap.LR_isWarning = true\n            return 'LR_WGSJ'\n        end\n        if DCSJTime == 0 and shootRelease > Minimap.autoCastTime and\n            shootRelease < shootTotal / 2 then\n            return 'LR_DCSJ'\n        end\n        if ASSJTime == 0 and shootRelease > Minimap.autoCastTime and\n            shootRelease < shootTotal / 2 then\n            return 'LR_ASSJ'\n        end\n    elseif shootTotal > 1.74 and not aura_env.config.weaving_shown then\n        -- 5611\n        Minimap.LR_Mode = '5611'\n        if Minimap.LR_CASTING_MS then\n            return\n        end\n        if ASSJTime == 0 and DCSJTime <= 1.5 and shootRelease >\n            Minimap.autoCastTime and shootRelease < shootTotal / 2 then\n            return 'LR_ASSJ'\n        end\n        if DCSJTime == 0 and shootRelease > Minimap.autoCastTime and\n            shootRelease < shootTotal / 2 then\n            return 'LR_DCSJ'\n        end\n        if ASSJTime >= 2.5 then\n            return\n        end\n        if Minimap.WGTotalTime < shootRelease then\n            return 'LR_WGSJ'\n        end\n        if shootRelease < Minimap.autoCastTime then\n            Minimap.LR_isWarning = true\n            return 'LR_WGSJ'\n        end\n    elseif shootTotal > 1.27 then\n        -- 11\n        Minimap.LR_Mode = '11'\n        if Minimap.WGTotalTime < shootRelease then\n            return 'LR_WGSJ'\n        end\n        if shootRelease < Minimap.autoCastTime then\n            Minimap.LR_isWarning = true\n            return 'LR_WGSJ'\n        end\n    elseif shootTotal > 1.09 then\n        -- 5911\n        Minimap.LR_Mode = '5911'\n        if Minimap.LR_CASTING_MS then\n            return\n        end\n        if DCSJTime == 0 then\n            return 'LR_DCSJ'\n        end\n        if ASSJTime == 0 and now - Minimap.LR_LAST_MS_TIME > 4 and now -\n            Minimap.LR_LAST_MS_TIME < 9 then\n            return 'LR_ASSJ'\n        end\n        return 'LR_WGSJ'\n    else\n        -- other\n        Minimap.LR_Mode = 'other'\n        if Minimap.WGTotalTime < shootRelease then\n            return 'LR_WGSJ'\n        end\n        if shootRelease < Minimap.autoCastTime then\n            Minimap.LR_isWarning = true\n            return 'LR_WGSJ'\n        end\n    end\n\n    return\nend\n",
