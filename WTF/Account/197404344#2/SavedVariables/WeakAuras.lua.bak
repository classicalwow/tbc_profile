		["面板属性 2"] = {
			["outline"] = "OUTLINE",
			["parent"] = "卢指导DK",
			["displayText"] = "%c",
			["customText"] = "function()\nend",
			["shadowYOffset"] = -1,
			["anchorPoint"] = "CENTER",
			["displayText_format_p_time_format"] = 0,
